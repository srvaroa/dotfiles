# image/*; ~/.mutt/muttimage.sh %s ; copiousoutput
image/gif; display %s
image/jpg; display %s; description=JPEG Image; nametemplate=%s.jpg
image/jpeg; display %s; description=JPEG Image; nametemplate=%s.jpg
text/text; vi '%s'; needsterminal; description=Text Text; nametemplate=%s.txt
application/pdf; evince '%s' -
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; libreoffice '%s'
